{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vmsampadf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"SalahAzureDatabrick_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'SalahAzureDatabrick'"
		},
		"event-trigger_properties_pipeline1_parameters_FileNamePL": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"event-trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1161f054-bb82-4ef5-a394-549d6d89cb36/resourceGroups/VMSAMP123_group/providers/Microsoft.Storage/storageAccounts/vmsampstorage"
		},
		"landingCheckTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1161f054-bb82-4ef5-a394-549d6d89cb36/resourceGroups/VMSAMP123_group/providers/Microsoft.Storage/storageAccounts/vmsampstorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vmsampadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"SalahAzureDatabrick_accessToken": {
						"value": "[parameters('SalahAzureDatabrick_accessToken')]"
					},
					"event-trigger_properties_pipeline1_parameters_FileNamePL": {
						"value": "[parameters('event-trigger_properties_pipeline1_parameters_FileNamePL')]"
					},
					"event-trigger_properties_typeProperties_scope": {
						"value": "[parameters('event-trigger_properties_typeProperties_scope')]"
					},
					"landingCheckTrigger_properties_typeProperties_scope": {
						"value": "[parameters('landingCheckTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}